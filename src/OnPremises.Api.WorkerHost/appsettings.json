{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Infrastructure.Broker.RabbitMq": "Debug",
      "MyCompany.WorkerHost": "Debug"
    }
  },
  "Hosts": {
    "ThisHost": {
      "Region": "australiaeast"
    },
    "EventNotificationApi": {
      "SubscribedHosts": "ApiHost1"
    },
    "ApiHost1": {
      "BaseUrl": "https://localhost:5001",
      "HMACAuthNSecret": "asecret"
    },
    "AncillaryApi": {
      "BaseUrl": "https://localhost:5001",
      "HMACAuthNSecret": "asecret"
    },
    "WebsiteHost": {
      "BaseUrl": "https://localhost:5101"
    },
    "PrivateInterHost": {
      "HMACAuthNSecret": "asecret"
    },
    "AzureFunctions": {
      "MaxDeliveryCountBeforeFailure": 5
    }
  },
  "RabbitMQ": {
    "ConnectionString": "amqp://guest:guest@localhost:5672/",
    "RequestedHeartbeat": 60,
    "AutomaticRecoveryEnabled": true,
    "NetworkRecoveryInterval": "00:00:10",
    "DefaultPrefetchCount": 10,
    "ConnectionName": "MyWorkerHostRelay",
    "MaxDeliveryCountBeforeFailure": 5,
    "CircuitBreakDurationMinutes": 2,
    "RetryPolicies": {
      "DefaultWorkQueuePolicy": {
        "RetryExchangeName": "direct.retry.exchange",
        "RetryExchangeType": "direct",
        "Attempts": [
          {
            "DelaySeconds": 5,
            "RoutingKeySuffix": "wait5s"
          },
          {
            "DelaySeconds": 30,
            "RoutingKeySuffix": "wait30s"
          },
          {
            "DelaySeconds": 300,
            "RoutingKeySuffix": "wait300s"
          }
        ],
        "FinalDlqSuffix": "final.dlq",
        "FinalDlxExchangeSuffix": "final.dlx",
        "FinalDlxExchangeType": "fanout"
      }
    },
    "Queues": {
      "Audits": "company_audits_queue",
      "Usages": "company_usages_queue",
      "DomainEventsApiHost1EndUsers": "domain_events_apihost1_endusers_sub_queue",
      "DomainEventsApiHost1Organizations": "domain_events_apihost1_orgs_sub_queue"
    },
    "Exchanges": {
      "DomainEventsExchange": {
        "Name": "company_domain_events_topic",
        "Type": "topic",
        "Durable": true,
        "AutoDelete": false,
        "Arguments": { }
      }
    },
    "Bindings": {
      "DomainEventsApiHost1EndUsers": {
        "Queue": "company_domain_events_apihost1_endusers_sub_queue",
        "Exchange": "company_domain_events_topic",
        "RoutingKey": "events.user.#"
      },
      "DomainEventsApiHost1Organizations": {
        "Queue": "company_domain_events_apihost1_orgs_sub_queue",
        "Exchange": "company_domain_events_topic",
        "RoutingKey": "events.organization.#"
      }
    }
  },
  "Platform": {
    "Setting1": "Value1"
  }
}
