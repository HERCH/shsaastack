{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Infrastructure.Broker.RabbitMq": "Debug",
      "OnPremises.Api.WorkerHost": "Debug"
    }
  },
  "RabbitMQ": {
    "ConnectionString": "amqp://guest:guest@localhost:5672/",
    "RequestedHeartbeat": 60,
    "AutomaticRecoveryEnabled": true,
    "NetworkRecoveryInterval": "00:00:10",
    "DefaultPrefetchCount": 10,
    "ConnectionName": "WorkerHostRelayService",
    "MaxDeliveryCountBeforeFailure": 5,
    "CircuitBreakDurationMinutes": 2,
    "RetryPolicies": {
      "DefaultWorkQueuePolicy": {
        "RetryExchangeName": "work.retry.exchange",
        "RetryExchangeType": "direct",
        "Attempts": [
          {
            "DelaySeconds": 5,
            "RoutingKeySuffix": "retry.5s"
          },
          {
            "DelaySeconds": 30,
            "RoutingKeySuffix": "retry.30s"
          },
          {
            "DelaySeconds": 300,
            "RoutingKeySuffix": "retry.5m"
          }
        ],
        "FinalDlqSuffix": "dlq",
        "FinalDlxExchangeSuffix": "dlx",
        "FinalDlxExchangeType": "fanout"
      },
      "DomainEventsPolicy": {
        "RetryExchangeName": "domain_events.retry.exchange",
        "RetryExchangeType": "direct",
        "Attempts": [
          {
            "DelaySeconds": 10,
            "RoutingKeySuffix": "retry.10s"
          },
          {
            "DelaySeconds": 60,
            "RoutingKeySuffix": "retry.60s"
          },
          {
            "DelaySeconds": 600,
            "RoutingKeySuffix": "retry.10m"
          }
        ],
        "FinalDlqSuffix": "dlq",
        "FinalDlxExchangeSuffix": "dlx",
        "FinalDlxExchangeType": "fanout"
      }
    },
    "Queues": {
      "DomainEvents_ApiHost1_EndUsers_Queue": "domain_events_ApiHost1-EndUsers_sub",
      "DomainEvents_ApiHost1_Organizations_Queue": "domain_events_ApiHost1-Organizations_sub",
      "DomainEvents_ApiHost1_Subscriptions_Queue": "domain_events_ApiHost1-Subscriptions_sub",
      "DomainEvents_ApiHost1_UserProfiles_Queue": "domain_events_ApiHost1-UserProfiles_sub"
    },
    "Exchanges": {
      "DomainEventsTopic": {
        "Name": "domain_events",
        "Type": "topic",
        "Durable": true,
        "AutoDelete": false,
        "Arguments": { }
      }
    },
    "Bindings": {
      "DomainEvents_ApiHost1_EndUsers": {
        "QueueConfigKey": "Queues:DomainEvents_ApiHost1_EndUsers_Queue",
        "ExchangeConfigKey": "Exchanges:DomainEventsTopic",
        "RoutingKey": "ApiHost1-EndUsers",
        "SubscriptionName": "ApiHost1-EndUsers"
      },
      "DomainEvents_ApiHost1_Organizations": {
        "QueueConfigKey": "Queues:DomainEvents_ApiHost1_Organizations_Queue",
        "ExchangeConfigKey": "Exchanges:DomainEventsTopic",
        "RoutingKey": "ApiHost1-Organizations",
        "SubscriptionName": "ApiHost1-Organizations"
      },
      "DomainEvents_ApiHost1_Subscriptions": {
        "QueueConfigKey": "Queues:DomainEvents_ApiHost1_Subscriptions_Queue",
        "ExchangeConfigKey": "Exchanges:DomainEventsTopic",
        "RoutingKey": "ApiHost1-Subscriptions",
        "SubscriptionName": "ApiHost1-Subscriptions"
      },
      "DomainEvents_ApiHost1_UserProfiles": {
        "QueueConfigKey": "Queues:DomainEvents_ApiHost1_UserProfiles_Queue",
        "ExchangeConfigKey": "Exchanges:DomainEventsTopic",
        "RoutingKey": "ApiHost1-UserProfiles",
        "SubscriptionName": "ApiHost1-UserProfiles"
      }
    }
  }
}